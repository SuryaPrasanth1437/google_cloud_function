from flask import Flask,make_response

app = Flask(__name__)

@app.route('/')
def hello_world():
    response = {
        "celebrityName": "hello world",
        "content": "hellow"
    }

    return make_response(json.dumps(response), 200, {'Content-Type': 'application/json'})

if __name__ == '__main__':
    app.run(debug=True)
